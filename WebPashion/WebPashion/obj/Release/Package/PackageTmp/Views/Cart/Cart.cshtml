        <h1>Giỏ hàng của bạn</h1>
        <table class="table">
            <tr class="table__row">
                <td class="table__content" data-heading="Player"><a style="font-weight:bold;" href="@Url.Action("Index","Home")"> <img style="width:40px" ; src="~/Content/images/return-shopping.png" />&#160;TIẾP TỤC MUA SẮM</a></td>
                <td class="table__content" data-heading="Seasons"></td>        
                <td class="table__content" data-heading="Points"></td>
                <td class="table__content" data-heading="Jersey Number"></td>
                <td class="table__content" data-heading="Teams"></td>
                <td class="table__content" data-heading="Teams"></td>
                <td class="table__content" data-heading="Career"></td>
                <td class="table__content" data-heading="Career"></td>
                <td class="table__content" data-heading="Teams"><a style="font-weight:bold;" href="@Url.Action("DeleteAllCart","Cart")"><img style="width:30px;" src="~/Content/images/IMG/delete-cart.png" />&#160;&#160;XÓA GIỎ HÀNG</a></td>
                <td class="table__content" data-heading="Career"><a style="font-weight:bold;" href="@Url.Action("Order","Cart")"><img style="width:30px;" src="~/Content/images/IMG/order-cart.png" />&#160;&#160;ĐẶT HÀNG</a></td>
            </tr>                  
            <tr style="font-weight:bold;">
                <th style="font-weight:bold;" class="table__heading">Mã sản phẩm</th>
                <th style="font-weight:bold;" class="table__heading">Tên sản phẩm</th>
                <th style="font-weight:bold;" class="table__heading">Ảnh</th>
                <th style="font-weight:bold;" class="table__heading">Số lượng</th>
                <th style="font-weight:bold;" class="table__heading">Size</th>
                <th style="font-weight:bold;" class="table__heading">Giá</th>
                <th style="font-weight:bold;" class="table__heading">Tiền</th>
                <th style="font-weight:bold;" class="table__heading">Chi tiết</th>
                <th style="font-weight:bold;" class="table__heading">Xóa</th>
                <th style="font-weight:bold;" class="table__heading">Cập nhật</th>
            </tr>
            @foreach (var item in Model)
            {
                <tr class="table__row" style="text-align:center">
                    <td class="table__content" data-heading="Player">@item.iID</td>
                    <td class="table__content" data-heading="Seasons">@item.sProductName</td>
                    <td class="table__content" data-heading="Points"><img style="width:50px;" src="~/Content/images/IMG/@item.sProductImage" /></td>
                    @using (Html.BeginForm("UpdateCart", "Cart", new { @id = item.iID }))
                    {
                        <td class="table__content" data-heading="Jersey Number"><input type="number" min="1" name="count" value="@item.iCount" style="border: 1px solid aliceblue   ; width: 35px; background-color: aliceblue;" /></td>
                        <td class="table__content" data-heading="Jersey Number">
                            <select style="width:50px" name="size">
                                <option selected disabled>Size</option>
                                <option selected hidden>@item.sSize</option>
                                <option>M</option>
                                <option>S</option>
                                <option>XL</option>
                                <option>XXL</option>
                            </select>
                        </td>
                        <td class="table__content" data-heading="Teams">@String.Format("{0:0,0}", item.iProductPrice)</td>
                        <td class="table__content" data-heading="Career">@String.Format("{0:0,0}", item.dMoney)</td>
                        <td class="table__content" data-heading="Teams"><a href="@Url.Action("GetDetail", "Home", new { id = item.iID })"><span style="font-size: 25px; color: #808080; "><i class="fas fa-info-circle"></i></span></a></td>
                        <td class="table__content" data-heading="Career"><a href="@Url.Action("DeleteCart", "Cart", new { @id = item.iID })"><span style="font-size: 25px; color: #808080;"><i class="far fa-trash-alt"></i></span></a></td>
                        <td class="table__content" data-heading="Career"><input style="border: none; background-color: rgb(199, 144, 6);color:white; border-radius: 13px; font-size: 16px; " type="submit" value="Cập nhật" /></td>
                    }
                </tr>
            }
            <tr class="table__row">
                <td class="table__content" data-heading="Player"></td>
                <td class="table__content" data-heading="Seasons"></td>
                <td class="table__content" data-heading="Points"></td>
                <td class="table__content" data-heading="Jersey Number"></td>
                <td class="table__content" data-heading="Teams"></td>
                <td class="table__content" data-heading="Teams">Số lượng sản phẩm: @ViewBag.SumCount</td>
                <td class="table__content" data-heading="Career">Tổng tiền: @String.Format("{0:0,0}", ViewBag.SumMoney) VNĐ</td>
            </tr>
        </table>

